{
    "name": "ALU de 8 bits con Carry Look-Ahead",
    "author": "Byron Rosales",
    "description": "ALU capaz de realizar suma, resta, AND y OR utilizando entradas de 4 bits extendidas a 8, con un sumador tipo look-ahead.",
    "inputs": [
        {
            "name": "A[3:0]",
            "from": 4,
            "to": 7
        },
        {
            "name": "B[3:0]",
            "from": 0,
            "to": 3
        },
        {
            "name": "SEL[2:0]",
            "from": 0,
            "to": 2
        }
    ],
    "outputs": [
        {
            "name": "RESULT[7:0]",
            "from": 0,
            "to": 7
        }
    ],
    "tags": [
        "alu",
        "carry-lookahead",
        "8-bit",
        "tiny tapeout",
        "byron"
    ]
}